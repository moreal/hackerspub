  "compilerOptions": {
    "lib": [
      "dom",
      "dom.asynciterable",
      "dom.iterable",
      "deno.ns",
      "deno.unstable"
    ],
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head"
    ]
  },
  "links": [
    "./patches/@deno/vite-plugin"
  ],
  "unstable": [
    "kv",
    "otel",
    "temporal",
    "sloppy-imports"
  ],
  "nodeModulesDir": "auto",
  "exclude": [
    ".github/",
    "drizzle/",
    "node_modules/",
    "web-next/node_modules/",
    "web-next/.output/",
    "web-next/.vinxi/",
    "patches/"
  ],
  "fmt": {
    "exclude": [
      "*.md",
      "*.yaml",
      "ai/prompts/",
      "graphql/locales/markdown/*.md",
      ".claude/commands/"
    ]
  },
  "lint": {
    "rules": {
      "exclude": [
        "require-await",
        "no-window",
        "no-window-prefix"
      ]
    }
  },
  "tasks": {
    "install": "deno cache */*.ts",
    "check": "deno fmt --check && deno lint && deno check .",
    "test": "deno test --env-file=.env.test --allow-all --parallel",
    "dev": "cd web && deno task dev",
    "build": "cd web && deno task build",
    "start": "cd web && deno task start",
    "drizzle-kit": "deno run -A drizzle-kit",
    "migrate": "deno run -A drizzle-kit migrate",
    "migrate:generate": "deno run -A drizzle-kit generate",
    "migrate:test": "deno run --env-file=.env.test -A drizzle-kit migrate",
    "keygen": "deno run --allow-read --allow-env scripts/keygen.ts",
    "addaccount": "deno run --allow-all --env-file=.env scripts/addaccount.ts",
    "hooks:install": "deno run --allow-read=deno.json,.git/hooks/ --allow-write=.git/hooks/ jsr:@hongminhee/deno-task-hooks",
    "hooks:pre-commit": "deno task check"
  }
}
